from sqlalchemy import create_engine, text

engine = create_engine('sqlite:///base.db', echo=True)
  
with engine.connect() as conn:
    conn.execute(text("CREATE TABLE tabla (x int, y int)"))
    conn.execute(
        text("INSERT INTO tabla (x, y) VALUES (:x, :y)"),
        [{"x": 1, "y": 1}, {"x": 2, "y": 4}],
    )
    conn.commit() 
